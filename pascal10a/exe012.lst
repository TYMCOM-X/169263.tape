    2	procedure testrealfile;
    3	 
    4	   begin
    5	   rewrite (realfile, 'test.tmp');
    6	   real_filename := filename (realfile);
    7	   realfile^ := 123.456e12;
    8	   put (realfile);
    9	   write (realfile, 987.654e3);
   10	   write (realfile, 56.789e2);
   11	   close (realfile);
   12	   reset (realfile, real_filename);
   13	   if realfile^ <> 123.456e12 then
   14	      begin
   15	      error (101);
   16	      writeln (tty, 'Wrong value in realfile: expected: ', 123.456e12,
   17	              ' got: ', realfile^);
   18	      writeln (tty, 'Value placed in file by "put", retrieved by "reset"');
   19	      end;
   20	   get (realfile);
   21	   if realfile^ <> 987.654e3 then
   22	      begin
   23	      error (102);
   24	      writeln (tty, 'Wrong value in realfile: expected: ', 987.654e3,
   25	             ' got: ', realfile^);
   26	      writeln (tty, 'Value placed in file by "write", retrieved by "get"');
   27	      end;
   28	   read (realfile, realvariable);
   29	   read (realfile, realvariable);
   30	   if realvariable <> 56.789e2 then
   31	      begin
   32	      error (103);
   33	      writeln (tty, 'Wrong value in realfile: expected: ', 56.789e2,
   34	             ' got: ', realvariable);
   35	      writeln (tty, 'Value placed in file by "write", retrieved by "read"');
   36	      end;
   37	   if not eof (realfile) then
   38	      begin
   39	      error (104);
   40	      writeln (tty, 'Didn''t get end of file on realfile.');
   41	      end;
   42	   close (realfile);
   43	   end;MODULE  EXE012       Test typed I/O.                                                   10-Aug-82 10:53
